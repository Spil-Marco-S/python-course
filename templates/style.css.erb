/* add your css style rules here */

h1 {
    font-size: 64px;
    font-weight: bold;
    margin-bottom: 0.5em;
    vertical-align: middle;
}
h2 {
    font-size: 56px;
    font-weight: bold;
    margin-bottom: 0.5em;
}

li {
    list-style-type: disc;
    list-style-position: outside;
    margin-left: 30px;
}

p {
    margin-top: 0.5em;
    margin-bottom: 0.5em;
}
pre, code {
    font-family: monospace;
    font-size: 16px;
    line-height: 1em;
    padding: 0;
}
pre {
    margin-left: 1em;
}
<%= content_for :css %>